pipeline {
    agent none
    stages {
        stage('Hello1') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello2') {
            steps {
                echo 'Hello World2'
            }
        }
        stage('Hello3') {
            steps {
                echo 'Hello World3'
                echo 'welcome to world5'
            }
        }
        stage('Hello4') {
            steps {
                echo 'Hello World4'
                echo 'welcome to world6'
            }
        }
    }
}
